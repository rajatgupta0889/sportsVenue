<div class="container">
	<h3>form</h3>
	<h3><a href="/form/new" class="btn btn-lg btn-primary">Create New Data</a></h3>
	<table class="table">
		<tr  class="active">
			<th>Id</th>
			<th>Name</th>
			<th>Facutly Address</th>
			<th>Owner First Name </th>
			<th>Owner Middle Name </th>
			<th>Owner Last Name </th>
			<th>Email</th>
			<th>Online Booking facility</th>
			<th>would they collaborate with mantra for online booking</th>
			<th>Image</th>
			<th>Regular Member email</th>
			<th>Regular Member Cont</th>
			<th>Sports Available</th>
			<th>Coaching Provided for sports </th>
			<th>Charges for Coching </th>
			<th>Instructors Email/Contact </th>
			<th>Sport require equipments</th>
			<th>Equipments Charges</th>
			<th>any Specific Timing</th>
			<th>If Yes </th>
			<th>Founding Facility 
			date</th>
			<th>Opening timing </th>
			<th>Closing timing </th>
			<th>Any Branches</th>
			<th>Branch Detail </th>
			<th>Does facility in other fields</th>
			<th>If yes </th>
			<th>if any multi field</th>
			<th>Number of Coaches</th>
			<th>Coach name</th>
			<th>Coach age </th>
			<th>Coach Exp </th>
			<th>Fee Structure</th>
			<th>Fee Structure 2</th>
			<th>Fee Structure 3</th>
			<th>any Other Requirements for Booking?</th>
			<th>Summary Facility</th>
			<th>Branch Own first name</th>
			<th>Middle Name</th>
			<th>Last Name </th>
			<th>Can ground be booked (Y/N)</th>
			<th>Fee for Ground Booking </th>

			<th></th>
			<th></th>
		</tr>

		<% _.each(form,function(form){%>
		<tr class="success" data-id="<%= form.id %>" data-model="form">
			<td><%= form.id %></td>
			<td><%= form.faculty_name %></td>
			<td><%= form.faculty_add %></td>
			<td><%= form.own_first_name %></td>
			<td><%= form.own_middle_name %></td>
			<td><%= form.own_last_name %></td>
			<td><%= form.own_email %></td>
			<td><%= form.on_book_faci%></td>
			<td><%= form.coll_mantra%></td>
			<td>  </td>
			<td><%= form.reg_membr_email %></td>
			<td><%= form.reg_membr_cont %></td>
			<td><%= form.sport_avail %></td>
			<td><%= form.coach_sport %></td>
			<td><%= form.choch_charg %></td>
			<td><%= form.instruc_email %></td>
			<td><%= form.req_equp %></td>
			<td><%= form.equp_charg %></td>
			<td><%= form.spec_time %></td>
			<td><%= form.specific_time %></td>
			<td><%= form.found_date %></td>
			<td><%= form.open_timing %></td>
			<td><%= form.close_timing %></td>
			<td><%= form.oth_branch %></td>
			<td><%= form.branch_detail %></td>
			<td><%= form.multi_field %></td>
			<td><%= form.multi_field_name %></td>
			<td><%= form.if_multi_field %></td>
			<td><%= form.no_coach %></td>
			<td><%= form.coach_name %> </td>
			<td><%= form.coach_age %></td>
			<td><%= form.coach_exp %></td>
			<td><%= form.fee_col_1 %></td>
			<td><%= form.fee_col_2 %></td>
			<td><%= form.fee_col_3 %></td>
			<td><%= form.oth_requir %></td>
			<td><%= form.summ %></td>
			<td><%= form.branch_own_first_name %></td>
			<td><%= form.branch_own_middle_name %></td>
			<td><%= form.branch_own_last_name %></td>
			<td><%= form.ground_book %></td>
			<td><%= form.fee_ground %></td>
			
			<td>
				<form action="/form/destroy/<%= form.id %>" method="post">
					<input type="hidden" name="method" value="delete"/>
					<input type="submit" class="btn btn-sm btn-danger" value="Del"/>
				</form>
			</td>
		</tr>
		<% }) %>	

	</table>
</div>